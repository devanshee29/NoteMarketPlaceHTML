                <!-- Basic profile Details  -->
                <div class="sub-heading">
                    <h4>Basic Profile Details</h4>
                </div>

                <div class="details">
                    <form>
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <div class="form-group text-left">
                                    <label>First Name *</label>
                                    <input type="text" class="form-control" name="first-name" placeholder="Enter your first name" required>
                                </div>
                            </div>

                            <div class="col-md-6 col-sm-6">
                                <div class="form-group text-left">
                                    <label>Last Name *</label>
                                    <input type="text" class="form-control" name="last-name" placeholder="Enter your last name" required>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <div class="form-group text-left">
                                    <label>Email *</label>
                                    <input type="email" class="form-control" name="email" placeholder="Enter your email address" required>
                                </div>
                            </div>

                            <div class="col-md-6 col-sm-6">
                                <div class="form-group text-left">
                                    <label>Date Of Birth</label>
                                    <input type="text" class="form-control" name="dob" placeholder="Enter your date of birth">
                                    <span class="field-icon"><img src="../../images/User-Profile/calendar.png"></span>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <div class="form-group text-left">
                                    <label>Gender</label>
                                    <select class="form-control">
                                        <option>Select your gender</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-6 col-sm-6">
                                <div class="form-group text-left ">

                                    <div class="col-md-12 col-sm-12 col-xs-12" style="padding-left: 0;">
                                        <label>Phone Number</label>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-3 col-sm-3 col-xs-3 tel01">
                                            <select class="form-control">
                                                <option>+91</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-9 col-md-9 col-xs-9 tel">
                                            <input type="tel" id="phone" name="phone" class="form-control" placeholder="Enter your phone number">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <label for="profile-picture" style="margin-top: 20px;">Profile Picture</label>
                                <div class="show-pic">
                                    <label for="file-input">
                                        <img src="../../images/User-Profile/upload.png" alt="Profile Picture">
                                        <p>Upload a picture</p>
                                    </label>
                                    <input type="file" class="form-control " id="file-input">
                                </div>
                            </div>

                        </div>
                    </form>
                </div>


