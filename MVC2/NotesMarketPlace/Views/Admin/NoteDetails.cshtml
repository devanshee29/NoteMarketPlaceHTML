@model IEnumerable<NotesMarketPlace.Models.SellerNote>
@{
    ViewBag.Title = "NoteDetails";
    Layout = "~/Views/Shared/Admin.cshtml";
}
<!-- Notes Details -->

    <section id="noteDetails">
        <div class="content-box-md content-box-sm content-box-xs">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="notes-details-heading">
                            <h6>Notes Details</h6>
                        </div>
                    </div>
                </div>

                <!-- note Details Left-Side -->
                <div class="row">
                    <div id="img-section" class="col-md-3  col-sm-6  col-xs-6">
                        <div class="note-details01-left">
                            <img src="~/Uploadimg/@ViewBag.Display_Picture" alt="book-cover">
                        </div>
                    </div>
                    <div id="book-description" class="col-md-3 col-sm-6 col-xs-6 padding-set">
                        <div class="sub-heading">
                            <h4 style="text-align:left;">@ViewBag.Title</h4>
                        </div>
                        <div class="note-details02-left">
                            <h6>@ViewBag.Category</h6>
                            <p>@ViewBag.Description</p>
                        </div>

                        <button class="btn btn-download" title="Download Notes Details"><a href='@Url.Action("Download", "RegisteredUser", new {path=ViewBag.path})' style="color:#fff;" class="btn-admin-download">Download</a></button>

                    </div>
                    <div id="book-other-info" class="col-md-6 col-sm-12 col-xs-12">
                        <!-- note Details Right-Side -->

                        <div id="notes-details-right">
                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right01">
                                    Institution:
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right02 padding-set">
                                    @ViewBag.University_Name
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right01">
                                    Country:
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right02 padding-set">
                                    @ViewBag.Country
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right01">
                                    Course Name:
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right02 padding-set">
                                    @ViewBag.Course
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right01">
                                    Course Code:
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right02 padding-set">
                                    @ViewBag.CourseCode
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right01">
                                    Professor:
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right02 padding-set">
                                    @ViewBag.Professor
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right01">
                                    Number of Pages:
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right02 padding-set">
                                    @ViewBag.Number_Of_Pages
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right01">
                                    Approved Date:
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 notes-details-right02 padding-set">
                                    @ViewBag.s1
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-4 col-sm-4 col-xs-5 notes-details-right01">
                                    Rating:
                                </div>
                                <div class="col-md-8  col-sm-8 col-xs-7 notes-details-right02 padding-set">
                                    100 Reviews


                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12 notes-details-review">
                                    5 Users marked this note as inappropriate
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <hr id="horizontal-line">


            <div class="container">
                <div class="row">
                    <div class=" col-md-6 col-sm-6">
                        <div class="notes-details-heading">
                            <h6>Notes Preview</h6>
                        </div>

                        <div id="Iframe-Cicis-Menu-To-Go">
                            <div class="responsive-wrapper responsive-wrapper-padding-bottom-90pct">
                                <embed src="~/Uploadimg/@ViewBag.NotesPreview" style="width:498px; height:450px;" />

                            </div>
                        </div>
                    </div>

                    <div class="col-md-6 col-sm-6 padding-set">
                        <div id="customer-review" class="notes-details-heading" style="margin-left: -30px">
                            <h6>Customer Review</h6>
                        </div>
                        <div class="container customer-review">
                            <!-- Customer Review 01 -->
                            <div class="row">
                                <div class="col-md-2 col-sm-2 col-xs-2">
                                    <img src="../../images/Homepage/client-1.jpg" alter="client" class="img-circle customer-review-id">

                                </div>

                                <div class="col-md-10 col-sm-10 col-xs-10">
                                    <h6>Richard Brown</h6>
                                    <div class="stars">
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-inactive"></i>
                                    </div>
                                    <img class="eye right delete" src="../../images/Dashboard/delete.png">
                                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Esse vero tempora eius, veniam laborum praesentium est molestias unde 1500.</p>
                                    <hr class="hr-customer-reviews">
                                </div>


                                <!-- Customer Review 02 -->
                                <div class="col-md-2 col-sm-2 col-xs-2">
                                    <img src="../../images/Homepage/client-2.jpg" alter="client" class="img-circle customer-review-id">

                                </div>

                                <div class="col-md-10 col-sm-10 col-xs-10">

                                    <h6>Alice Ortiaz</h6>
                                    <div class="stars">
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-inactive"></i>
                                    </div>
                                    <img class="eye right delete" src="../../images/Dashboard/delete.png">
                                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Esse vero tempora eius, veniam laborum praesentium est molestias unde 1500.</p>
                                    <hr class="hr-customer-reviews">
                                </div>


                                <!-- Customer Review 03 -->
                                <div class="col-md-2 col-sm-2 col-sm-2 col-xs-2">
                                    <img src="../../images/Homepage/client-3.jpg" alter="client" class="img-circle customer-review-id">

                                </div>

                                <div class="col-md-10 col-sm-10 col-sm-10 col-xs-10">
                                    <h6>Sara Passmore</h6>
                                    <div class="stars">
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-active"></i>
                                        <i class="fa fa-star rating-star-inactive"></i>
                                    </div>
                                    <img class="eye right delete" src="../../images/Dashboard/delete.png">
                                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Esse vero tempora eius, veniam laborum praesentium est molestias unde 1500.</p>
                                </div>


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
